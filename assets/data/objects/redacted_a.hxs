registerClass('backend.AssetPaths');
registerClass('flixel.FlxG');
registerClass('objects.dialogue.Typer');

this.loadGraphic(AssetPaths.sprite(this.name));
this.immovable = true;
this.solid = true;

function interact()
{
	this.interacting = true;

	var typer:Typer = new Typer({name: 'Wingdings', size: 24}, {name: 'txt1', volume: 1}, 3, 1.6);

	FlxG.state.startDialogue([{typer: typer, text: '* [redacted]'}], function()
	{
		this.interacting = false;
	});
}
